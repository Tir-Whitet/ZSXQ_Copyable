{
    "manifest_version": 3,
    "name": "ZSXQ Copy Unlocker",
    "version": "1.1",
    "content_scripts": [{
      "matches": ["*://*.zsxq.com/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "css": ["override.css"],
      "all_frames": true
    }],
    "host_permissions": [
      "*://*.zsxq.com/*"
    ],
    "permissions": [
      "scripting"
    ]
  }